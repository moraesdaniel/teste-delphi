CREATE TABLE DEPENDENTE (
	ID			INTEGER			NOT NULL,
	NOME		VARCHAR(200)	NOT NULL,
	CALCULAINSS	CHAR(1)			DEFAULT 'F' 	NOT NULL,
	CALCULAIR	CHAR(1)			DEFAULT 'F' 	NOT NULL
)

ALTER TABLE DEPENDENTE ADD CONSTRAINT
PK_DEPENDENTE PRIMARY KEY (ID)

CREATE GENERATOR GEN_ID_DEPENDENTE


CREATE TABLE FUNCIONARIO_DEPENDENTE (
	IDFUNCIONARIO	INTEGER		NOT NULL,
	IDDEPENDENTE	INTEGER 	NOT NULL
)

ALTER TABLE FUNCIONARIO_DEPENDENTE ADD CONSTRAINT PK_FUNC_DEPEND
PRIMARY KEY (IDFUNCIONARIO, IDDEPENDENTE)

ALTER TABLE FUNCIONARIO_DEPENDENTE ADD CONSTRAINT FK_FD_FUNC 
FOREIGN KEY (IDFUNCIONARIO) REFERENCES FUNCIONARIO (ID)

ALTER TABLE FUNCIONARIO_DEPENDENTE ADD CONSTRAINT FK_FD_DEPEND 
FOREIGN KEY (IDDEPENDENTE) REFERENCES DEPENDENTE (ID)